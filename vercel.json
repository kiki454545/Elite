{
  "buildCommand": "npm run build",
  "framework": "nextjs",
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  },
  "crons": [
    {
      "path": "/api/cron/reset-weekly-views",
      "schedule": "0 0 * * 1"
    },
    {
      "path": "/api/cron/increment-views",
      "schedule": "0 3 * * *"
    }
  ]
}
